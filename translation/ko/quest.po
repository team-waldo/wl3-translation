msgid " UNUSED"
msgstr " 사용 안 함"

msgctxt "mission_a1001_dontyoubemyneighbor/9"
msgid " UNUSED"
msgstr " 사용 안 함"

msgctxt "mission_a1001_dontyoubemyneighbor/10"
msgid " UNUSED"
msgstr " 사용 안 함"

