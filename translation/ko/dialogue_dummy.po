msgstr ""

msgctxt "d1001_follower_bearsuitvinters/73"
msgid "\"Hold your fire, Rangers! It looks like we're safe around this bear.\""
msgstr ""

msgctxt "d1001_follower_bearsuitvinters/74"
msgid "Stop staring so closely at the 'bear.'"
msgstr ""

msgctxt "d1001_follower_bearsuitvinters/75"
msgid ""
